{
  "name": "webpack5-demo",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "author": "Better",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "test": "echo \"warn: no test specified\"",
    "watch": "webpack --watch",
    "start": "webpack serve --node-env development --config webpack.dev.js",
    "dev": "webpack serve --node-env development --config webpack.dev.js",
    "build": "webpack --node-env production --config webpack.prod.js && npm run getGitInfo",
    "start:server": "http-server dist",
    "server": "node server.js",
    "getGitInfo": "node scripts/git/useNodeGetGitInfo.js",
    "eslint": "eslint --fix src/**/* --fix --ext .js,.jsx,.ts,.tsx",
    "lint": "git add . && npm run lint:pre-commit",
    "lint:pre-commit": "lint-staged --no-stash",
    "commit-msg": "node scripts/git/verifyCommitMsg.js",
    "prepare": "husky install"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/git/verifyCommitMsg.js"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue}": "eslint --fix",
    "src/**/*": "eslint --fix src/**/* --fix --ext .js,.jsx,.ts,.tsx"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cache-loader": "^4.1.0",
    "css-loader": "^5.1.1",
    "csv-loader": "^3.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "eslint-webpack-plugin": "^2.5.4",
    "express": "^4.17.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.0",
    "http-server": "^0.12.3",
    "husky": "^6.0.0",
    "markdown-loader": "^6.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "prettier": "2.3.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "webpack": "^5.27.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "workbox-webpack-plugin": "^6.1.1",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.21",
    "whatwg-fetch": "^3.6.2"
  }
}
